###ENV_PART###
CONSOLE_API_URL=https://{{EASY_SUFIX}}{{EASY_DIFY_API}}.{{EASY_DOMAIN}}
APP_API_URL=https://{{EASY_SUFIX}}{{EASY_DIFY_API}}.{{EASY_DOMAIN}}
SENTRY_DSN=
NEXT_TELEMETRY_DISABLED=0

###ENV_PART###
API_KEY=sk-{{EASY_KEY}}
GIN_MODE=release
WORKER_TIMEOUT=15
SANDBOX_PORT=8194

###ENV_PART###
MODE=api
SECRET_KEY=sk-{{EASY_KEY}}
LOG_LEVEL=WARNING

APP_WEB_URL=https://{{EASY_SUFIX}}{{EASY_DIFY}}.{{EASY_DOMAIN}}
CONSOLE_WEB_URL=https://{{EASY_SUFIX}}{{EASY_DIFY}}.{{EASY_DOMAIN}}
APP_API_URL=https://{{EASY_SUFIX}}{{EASY_DIFY_API}}.{{EASY_DOMAIN}}
CONSOLE_API_URL=https://{{EASY_SUFIX}}{{EASY_DIFY_API}}.{{EASY_DOMAIN}}
SERVICE_API_URL=https://{{EASY_SUFIX}}{{EASY_DIFY_API}}.{{EASY_DOMAIN}}

FILES_URL=
INIT_PASSWORD=
DEPLOY_ENV=PRODUCTION
ETL_TYPE=dify

# Postgres
MIGRATION_ENABLED=true
DB_USERNAME=postgres
DB_PASSWORD={{EASY_PASSWORD}}
DB_HOST={{EASY_PROJECT}}_postgres
DB_PORT=5432
DB_DATABASE=dify

# Redis
REDIS_HOST={{EASY_PROJECT}}_redis
REDIS_PORT=6379
REDIS_USERNAME=default
REDIS_PASSWORD={{EASY_PASSWORD}}
REDIS_USE_SSL=false
REDIS_DB={{EASY_REDIS_DIFY}}
CELERY_BROKER_URL=redis://default:{{EASY_PASSWORD}}@{{EASY_PROJECT}}_redis:6379/{{EASY_REDIS_DIFY_CELERY}}

# Cors
WEB_API_CORS_ALLOW_ORIGINS=*
CONSOLE_CORS_ALLOW_ORIGINS=*

# Storage
STORAGE_TYPE=s3 #local
STORAGE_LOCAL_PATH=storage
S3_ENDPOINT=https://{{EASY_SUFIX}}{{EASY_MINIO_S3}}.{{EASY_DOMAIN}}
S3_BUCKET_NAME=dify
S3_REGION=us-east-1
S3_ACCESS_KEY={{EASY_MINIO_KEY}}
S3_SECRET_KEY={{EASY_MINIO_ACCESS}}

# Sandbox
CODE_EXECUTION_ENDPOINT=http://{{EASY_PROJECT}}_dify_sandbox:8194
CODE_EXECUTION_API_KEY=sk-{{EASY_KEY}}
CODE_MAX_NUMBER=9223372036854775807
CODE_MIN_NUMBER=-9223372036854775808
CODE_MAX_STRING_LENGTH=80000
TEMPLATE_TRANSFORM_MAX_LENGTH=80000
CODE_MAX_STRING_ARRAY_LENGTH=30
CODE_MAX_OBJECT_ARRAY_LENGTH=30
CODE_MAX_NUMBER_ARRAY_LENGTH=1000

# Weaviate
VECTOR_STORE=weaviate
WEAVIATE_ENDPOINT=http://{{EASY_PROJECT}}_weaviate:8080
WEAVIATE_API_KEY={{EASY_KEY}}
WEAVIATE_CLIENT_TIMEOUT=20

# Smtp
MAIL_TYPE=smtp
MAIL_DEFAULT_SEND_FROM={{EASY_SMTP_SENDER}}
SMTP_SERVER={{EASY_SMTP_HOST}}
SMTP_PORT={{EASY_SMTP_PORT}}
SMTP_USERNAME={{EASY_SMTP_USERNAME}}
SMTP_PASSWORD={{EASY_SMTP_PASSWORD}}

# Sentry
SENTRY_DSN=
SENTRY_TRACES_SAMPLE_RATE=1.0
SENTRY_PROFILES_SAMPLE_RATE=1.0