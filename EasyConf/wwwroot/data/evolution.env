# Url da Evolution API
SERVER_URL=https://{{EASY_SUFIX}}{{EASY_EVOLUTION}}.{{EASY_DOMAIN}}

# Dados de Autenticacao
AUTHENTICATION_TYPE=apikey
AUTHENTICATION_API_KEY={{EASY_KEY}}
AUTHENTICATION_EXPOSE_IN_FETCH_INSTANCES=true
CORS_ORIGIN=*
CORS_METHODS=POST,GET,PUT,DELETE
CORS_CREDENTIALS=true

# Configuracoes
LANGUAGE=pt-BR
CONFIG_SESSION_PHONE_CLIENT={{EASY_NAME}}
CONFIG_SESSION_PHONE_NAME=chrome    
CONFIG_SESSION_PHONE_VERSION=2.3000.1015901307

# Configuracoe Whatsapp Business Cloud (Api Oficial)
WA_BUSINESS_TOKEN_WEBHOOK=evolution
WA_BUSINESS_URL=https://graph.facebook.com
WA_BUSINESS_VERSION=v20.0
WA_BUSINESS_LANGUAGE=pt_BR

# Sobre o QR-Code
QRCODE_LIMIT=60
QRCODE_COLOR=#47A7F6

# Ativar Postgres
DATABASE_ENABLED=true
DATABASE_PROVIDER=postgresql
DATABASE_CONNECTION_URI=postgresql://postgres:{{EASY_PASSWORD}}@{{EASY_PROJECT}}_pgbouncer:5432/evolution?schema=public&pgbouncer=true
DATABASE_CONNECTION_CLIENT_NAME=evolution
DATABASE_SAVE_DATA_INSTANCE=true
DATABASE_SAVE_DATA_NEW_MESSAGE=true
DATABASE_SAVE_MESSAGE_UPDATE=true
DATABASE_SAVE_DATA_CONTACTS=true
DATABASE_SAVE_DATA_CHATS=true

# Ativar MinIO
S3_ENABLED=true
S3_ACCESS_KEY={{EASY_MINIO_KEY}}
S3_SECRET_KEY={{EASY_MINIO_ACCESS}}
S3_BUCKET=evolution
S3_PORT=443
S3_ENDPOINT={{EASY_SUFIX}}{{EASY_MINIO_S3}}.{{EASY_DOMAIN}}
S3_USE_SSL=true    

# Ativar Redis
CACHE_REDIS_ENABLED=true
CACHE_REDIS_URI=redis://default:{{EASY_PASSWORD}}@{{EASY_PROJECT}}_redis:6379/{{EASY_REDIS_EVOLUTION}}
CACHE_REDIS_PREFIX_KEY=evolution
CACHE_REDIS_SAVE_INSTANCES=false
CACHE_LOCAL_ENABLED=false

# Integracoes
OPENAI_ENABLED=true
DIFY_ENABLED=true
TYPEBOT_ENABLED=true
TYPEBOT_API_VERSION=latest
TYPEBOT_KEEP_OPEN=false
TYPEBOT_SEND_MEDIA_BASE64=true

# Ativar Chatwoot
CHATWOOT_ENABLED=true
CHATWOOT_MESSAGE_READ=true
CHATWOOT_MESSAGE_DELETE=true
CHATWOOT_IMPORT_DATABASE_CONNECTION_URI=postgresql://postgres:{{EASY_PASSWORD}}@{{EASY_PROJECT}}_pgbouncer:5432/chatwoot?sslmode=disable
CHATWOOT_IMPORT_PLACEHOLDER_MEDIA_MESSAGE=false

# Configuracoes das instancias
DEL_INSTANCE=false
DEL_TEMP_INSTANCES=false

# Logs
LOG_LEVEL=ERROR,WARN,DEBUG,INFO,LOG,DARK,WEBHOOKS
LOG_COLOR=true
LOG_BAILEYS=error
TELEMETRY=false
TELEMETRY_URL=

# Ativar RabbitMQ
RABBITMQ_ENABLED=false
RABBITMQ_URI=amqp://{{EASY_USER}}:{{EASY_PASSWORD}}@{{EASY_PROJECT}}_rabbitmq:5672/default
RABBITMQ_EXCHANGE_NAME=evolution
RABBITMQ_GLOBAL_ENABLED=false
RABBITMQ_EVENTS_APPLICATION_STARTUP=false
